<!DOCTYPE html>
<html>
<head>
  <title>DevFiltr</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<%= render :partial => "layouts/nav_bar" %>
	
	<% if notice %>
	<p class="notice"><%= notice %></p>
	<% end %>
	<% if alert %>
	<p class="alert"><%= alert %></p>
	<% end %>
	<div id="content" class="container">
		<%= yield %>
	</div>
